{
    "name": "venveo/hubspot-toolbox",
    "description": "Turnkey HubSpot integration for CraftCMS",
    "type": "craft-plugin",
    "version": "1.0.0-beta",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "hubspot toolbox"
    ],
    "support": {
        "docs": "https://github.com/venveo/hubspot-toolbox/blob/master/README.md",
        "issues": "https://github.com/venveo/hubspot-toolbox/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Venveo",
            "homepage": "https://venveo.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0",
        "ryanwinchester/hubspot-php": "~1.0",
        "flipboxdigital/oauth2-hubspot": "^1.0"
    },
    "autoload": {
        "psr-4": {
          "venveo\\hubspottoolbox\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/venveo/oauth2-hubspot"

        }
    ],
    "extra": {
        "name": "HubSpot Toolbox",
        "handle": "hubspot-toolbox",
        "hasCpSettings": true,
        "hasCpSection": true,
        "changelogUrl": "https://raw.githubusercontent.com/venveo/hubspot-toolbox/master/CHANGELOG.md",
        "components": {
            "hubSpotService": "venveo\\hubspottoolbox\\services\\HubSpotService"
        },
        "class": "venveo\\hubspottoolbox\\HubspotToolbox"
    }
}
