{
    "name": "venveo/craft-hubspot-toolbox",
    "description": "Turnkey HubSpot integration for CraftCMS",
    "type": "craft-plugin",
    "version": "1.0.0-beta3",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "hubspot toolbox"
    ],
    "support": {
        "docs": "https://github.com/venveo/craft-hubspot-toolbox/blob/master/README.md",
        "issues": "https://github.com/venveo/craft-hubspot-toolbox/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Venveo",
            "homepage": "https://venveo.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0",
        "hubspot/hubspot-php": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "venveo\\hubspottoolbox\\": "src/"
        }
    },
    "extra": {
        "name": "HubSpot Toolbox",
        "handle": "hubspot-toolbox",
        "changelogUrl": "https://raw.githubusercontent.com/venveo/craft-hubspot-toolbox/master/CHANGELOG.md",
        "class": "venveo\\hubspottoolbox\\HubSpotToolbox"
    }
